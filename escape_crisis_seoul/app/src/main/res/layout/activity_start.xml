<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#ffc0cb"
    tools:context=".StartActivity">

    <ImageButton
        android:id="@+id/dkvkdyButton"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="50dp"
        android:layout_marginTop="50dp"
        android:src="@drawable/dkvkdy100px" />

    <ImageButton
        android:id="@+id/antjdnjdyButton"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="200dp"
        android:layout_marginTop="50dp"
        android:src="@drawable/antjdnjdy100px" />

    <ImageButton
        android:id="@+id/dmdrmqdmdrmqButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="50dp"
        android:layout_marginTop="200dp"
        android:src="@drawable/dmdrmqdmdrmq250px" />

    <Button
        android:id="@+id/call_button"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="930dp"
        android:text="전화걸기" />



    <Button
        android:id="@+id/map_button"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="1230dp"
        android:text="맵!!" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/dkvkdyButton"
        android:layout_below="@+id/dkvkdyButton"
        android:textSize="20sp"
        android:text="아파요!!" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/antjdnjdyButton"
        android:layout_below="@+id/antjdnjdyButton"
        android:textSize="20sp"
        android:text="무서워요ㅜㅜ" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/dmdrmqdmdrmqButton"
        android:layout_below="@+id/dmdrmqdmdrmqButton"
        android:textSize="20sp"
        android:text="응급응급!!" />

</RelativeLayout>